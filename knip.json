{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "**/*.{test,spec}.?(c|m)[jt]s?(x)",
    "**/test/**/*.?(c|m)[jt]s?(x)",
    "eslint.config.mjs",
    "packages/container/src/font/font-loader.ts",
    "packages/container/src/state/user-schema.ts",
    "packages/pangram/src/types.ts",
    "src/**/example*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,vue}",
    "src/cli.ts!",
    "src/index.ts!",
    "src/types.ts",
    "{scripts,perf}/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"
  ],
  "ignore": ["**/*.d.+([a-zA-Z0-9]).ts", "**/*.d.ts", "lib/**", "packages/container/test/**"],
  "ignoreBinaries": ["ncu"],
  "ignoreDependencies": [
    "pangram",
    "@commitlint/cli",
    "@microsoft/api-extractor",
    "eslint-config-escapace",
    "prettier",
    "syncpack"
  ],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "lefthook": {
    "config": []
  },
  "project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,vue}"]
}
