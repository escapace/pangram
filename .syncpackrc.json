{
  "dependencyTypes": [
    "dev",
    "overrides",
    "peer",
    "pnpmOverrides",
    "prod",
    "resolutions",
    "local",
    "engines"
  ],
  "customTypes": {
    "engines": {
      "path": "engines",
      "strategy": "versionsByName"
    }
  },
  "versionGroups": [
    {
      "packages": ["**"],
      "dependencies": [
        "@escapace/web-font",
        "@escapace/web-font-container",
        "@escapace/web-font-metric"
      ],
      "isIgnored": true
    }
  ],
  "semverGroups": [
    {
      "dependencies": ["**"],
      "dependencyTypes": ["engines", "peer"],
      "packages": ["**"],
      "range": ">="
    },
    {
      "dependencies": ["**"],
      "dependencyTypes": ["dev"],
      "packages": ["**"],
      "range": ""
    },
    {
      "dependencies": ["**"],
      "dependencyTypes": ["prod"],
      "packages": ["**"],
      "range": "^"
    }
  ],
  "source": ["package.json", "packages/*/package.json"]
}
